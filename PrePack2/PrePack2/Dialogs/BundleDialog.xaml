<Window x:Name="BundleDialogWindow" x:Class="BundleDialog"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PrePack2"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="BundleDialog" WindowStyle="None" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterOwner" Background="{x:Null}">
    <Grid Background="#FF0046FF">
        <Grid.RowDefinitions>
            <RowDefinition Height="13*"/>
            <RowDefinition Height="2*"/>
        </Grid.RowDefinitions>
        <Button x:Name="SubmitButton" Content="Button" Margin="112.5,10" Grid.Row="1" RenderTransformOrigin="0.386,0.597"/>
        <ListBox x:Name="SkuBox" HorizontalAlignment="Left" Height="260" VerticalAlignment="Top" Width="300" Background="{x:Null}" Foreground="White"/>
    </Grid>
</Window>
