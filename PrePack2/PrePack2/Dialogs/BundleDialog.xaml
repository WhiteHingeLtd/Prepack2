<Window x:Name="BundleDialogWindow" x:Class="BundleDialog"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PrePack2"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="BundleDialog" WindowStyle="None" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterOwner" Background="{x:Null}" Height="479.2" Width="854">
    <Grid Background="#FF0046FF">
        <Grid.RowDefinitions>
            <RowDefinition Height="13*"/>
            <RowDefinition Height="2*"/>
        </Grid.RowDefinitions>
        <Button x:Name="SubmitButton" Content="Choose" Margin="118,6,119,6" Grid.Row="1" RenderTransformOrigin="0.386,0.597" FontSize="18" VerticalAlignment="Center"/>
        <ListBox x:Name="SkuBox" Background="{x:Null}" Foreground="White" FontSize="36" HorizontalContentAlignment="Center" FontWeight="Bold"/>
    </Grid>
</Window>
