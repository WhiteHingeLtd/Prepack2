<Window x:Class="MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PrePack2"
        mc:Ignorable="d"
        Closing="Main_FormClosing"
        Title="MainWindow" Height="900" Width="1900">
    <Grid Height="890" VerticalAlignment="Top" Margin="0,0,150,-21">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="480*"/>
            <ColumnDefinition Width="93*"/>
            <ColumnDefinition Width="93*"/>
            <ColumnDefinition Width="134*"/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="69*"/>
            <RowDefinition Height="376*"/>
        </Grid.RowDefinitions>
        <Grid Grid.ColumnSpan="4" Height="27.212" VerticalAlignment="Top" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin"/>
